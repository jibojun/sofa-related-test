<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:sofa="http://code.alibabatech.com/schema/dubbo"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <sofa:application name="sofaRpcTestService"/>
    <sofa:annotation package="com.sofarpctest.service.impl"/>
    <sofa:protocol name="bolt" host="127.0.0.1" port="20891"/>
    <sofa:protocol name="rest" host="127.0.0.1" port="20892"/>
    <sofa:protocol name="dubbo" host="127.0.0.1" port="20893"/>

    <context:component-scan base-package="com.sofarpctest.service.impl"/>
</beans>